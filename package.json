{
  "name": "@kuali/red-rover",
  "version": "0.0.0",
  "description": "A utility library to handle distributed redis subscriptions.",
  "main": "index.js",
  "scripts": {
    "test:unit": "istanbul cover _mocha",
    "test:lint": "eslint . --ignore-path .gitignore"
  },
  "eslintConfig": {
    "extends": [
      "ksmithut",
      "ksmithut/node",
      "ksmithut/es6-node",
      "ksmithut/mocha"
    ],
    "rules": {
      "no-underscrore-dangle": [
        "allow"
      ],
      "brace-style": [
        "error",
        "stroustrup",
        {
          "allowSingleLine": true
        }
      ],
      "object-property-newline": [
        "error",
        {
          "allowMultiplePropertiesPerLine": true
        }
      ]
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-spies": "^0.7.1",
    "eslint": "^3.7.1",
    "eslint-config-ksmithut": "^2.2.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2"
  },
  "dependencies": {
    "redis": "^2.6.2",
    "uuid": "^2.0.3"
  }
}
